{
  "env": {
    "browser": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": [
    "airbnb-base",
    "plugin:react/recommended",
    "eslint-config-prettier"
  ],
  "rules": {
    "no-console": "off"
  },
  "plugins": ["react", "import"]
}
